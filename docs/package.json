{
  "name": "smally-docs",
  "version": "1.0.0",
  "description": "Smally API Documentation",
  "scripts": {
    "docusaurus": "docusaurus",
    "generate-openapi": "cd .. && cargo run --bin generate_openapi",
    "gen-api-docs": "docusaurus gen-api-docs smally",
    "clean-api-docs": "docusaurus clean-api-docs smally",
    "prebuild": "bun run generate-openapi && bun run gen-api-docs",
    "prestart": "bun run generate-openapi && bun run gen-api-docs",
    "start": "docusaurus start",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids"
  },
  "dependencies": {
    "@docusaurus/core": "^3.9.2",
    "@docusaurus/faster": "^3.9.2",
    "@docusaurus/preset-classic": "^3.9.2",
    "@stoplight/elements": "^9.0.11",
    "buffer": "^6.0.3",
    "clsx": "^2.0.0",
    "docusaurus-plugin-openapi-docs": "^4.5.1",
    "docusaurus-theme-openapi-docs": "^4.5.1",
    "prism-react-renderer": "^2.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "url": "^0.11.4"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
